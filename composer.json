{
  "name": "mikaelmattsson/wordpress-plugin-boilerplate",
  "description": "A boilerplate for WordPress plugins that uses composer.",
  "license": "GPLv2",
  "keywords": ["wordpress", "plugin", "boilerplate"],
  "authors": [
    {
      "name": "Name Lastname",
      "email": "example@example.com"
    }
  ],
  "require": {
    "php": ">=5.4.7"
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "2.0.*@dev"
  },
  "autoload": {
    "psr-4": {
      "CompanyName\\PluginName\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  }
}
